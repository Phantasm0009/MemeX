{
  "name": "italian-meme-dashboard",
  "version": "1.0.0",
  "description": "Dashboard for Italian Meme Stock Exchange",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "build": "npm install --production",
    "build-css": "postcss public/css/dashboard-built.css -o public/css/dashboard-compiled.css",
    "watch-css": "postcss public/css/dashboard-built.css -o public/css/dashboard-compiled.css --watch"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
  "sitemap": "^7.1.2",
    "postcss-cli": "^11.0.1",
    "socket.io": "^4.7.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "dashboard",
    "meme-stocks",
    "italian",
    "trading"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12"
  }
}
